<!-- maze -->

<Environment>
<camtrans>18.821295 16.280615 52.393200</camtrans>
<camrotationaxis>-0.999964 0.000198 -0.008424 176.466456</camrotationaxis>
<camfocal>52.445621</camfocal>

   <Robot name="ecroSim" file="ecro.robot.xml">
       <RotationAxis>0 0 0 0</RotationAxis>
       <translation>3 3 0.01</translation>
   </Robot>

   <Robot name="ecroSim2" file="ecro.robot.xml">
       <RotationAxis>0 0 0 0</RotationAxis>
       <translation>3 8 0.01</translation>
   </Robot>

   <KinBody name="ProjectRoom">
    <!-- Floor Dimension(m): 40 x 40 x 0.1 -->
    <Body name="Floor" type="static">
	  <Geom type="box">
	    <Extents>10 10 0.05</Extents>
	    <Translation>10 10 0</Translation>
	    <diffuseColor>0.9 0.8 0.6</diffuseColor>
	  </Geom>
    </Body>
    
	 <physicsengine type="ode">
	  <odeproperties>
	   <friction>.5</friction>
	   <gravity>0 0 -9.8</gravity>
	   <selfcollision>1</selfcollision>
	   <erp>.5</erp>
	   <cfm>.000001</cfm>
	   <dcontactapprox>1</dcontactapprox>
	  </odeproperties>
	 </physicsengine>
	 
    <!-- Walls Limits-->
    <Body name="FWL" type="static">
      <offsetfrom>Floor</offsetfrom>
	  <Geom type="box">
	    <Extents>1 10 0.75</Extents>
	    <Translation>0 10 0.75</Translation>
	    <diffuseColor>0.1 0.1 0.1</diffuseColor>
	  </Geom>
    </Body>

    <Body name="FWR" type="static">
      <offsetfrom>Floor</offsetfrom>
	  <Geom type="box">
	    <Extents>1 10 0.75</Extents>
	    <Translation>20 10 0.75</Translation>
	    <diffuseColor>0.1 0.1 0.1</diffuseColor>
	  </Geom>
    </Body>

    <Body name="FWB" type="static">
      <offsetfrom>Floor</offsetfrom>
	  <Geom type="box">
	    <Extents>10 1 0.75</Extents>
	    <Translation>10 0 0.75</Translation>
	    <diffuseColor>0.1 0.1 0.1</diffuseColor>
	  </Geom>
    </Body>
    
    <Body name="FWT" type="static">
      <offsetfrom>Floor</offsetfrom>
	  <Geom type="box">
	    <Extents>10 1 0.75</Extents>
	    <Translation>10 20 0.75</Translation>
	    <diffuseColor>0.1 0.1 0.1</diffuseColor>
	  </Geom>
    </Body>
    
    <!-- others walls -->

    <Body name="OtherWall" type="static">
      <offsetfrom>Floor</offsetfrom>
	  <Geom type="box">
	    <Extents>7 1 0.75</Extents>
	    <Translation>8 5 0.75</Translation>
	    <diffuseColor>0.5 1 0.07</diffuseColor>
	  </Geom>
    </Body>
    
  </KinBody>



</Environment>

